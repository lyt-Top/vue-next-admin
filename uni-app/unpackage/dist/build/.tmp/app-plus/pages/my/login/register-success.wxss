page {
  background-color: #f4f8fe;
  height: 100%;
}
.no-more {
  height: 100%;
}
.no-more-img {
    height: 600rpx;
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    padding: 100rpx 0 0 0;
}
.no-more-img .img {
      margin: auto;
      width: 203rpx;
      height: 190rpx;
}
.no-more-img .msg {
      font-size: 32rpx;
      color: #778190;
      text-align: center;
}
.no-more-img .time-box {
      font-size: 32rpx;
      text-align: center;
      padding: 100rpx 0;
}
.no-more-img .time-box .time {
        color: #198cff;
}

